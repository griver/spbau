package homework2;

public class IntegralType implements  ASTTreeNode {
    private Integer value = null;
    private String type = "IntegralType";

    public IntegralType() {}

    public IntegralType(int val) {
        value = val;
    }
    
    public String getType() {
        return type;
    } 
    
    public ASTTreeNode reduse() {
        return this;
    } 
    
}
